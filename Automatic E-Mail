#This program has the function of automatically sending an email
#This code was made in python
#First download the library smtplib using the command "pip install smtplib"

import smtplib

email_from = '' #Put your e-mail in the ' '
email_to = '' #Put destiny e-mail in the ' '
smtp = 'smtp.gmail.com'
server = smtplib.SMTP(smtp, 587)
server.starttls()
server.login(email_from, '') #Put your password in the ' '
msg = ''' ''' #Put your password in the ' '
server.sendmail(email_from, email_to, msg)
server.quit()
print("Code Finished")
